-var titulo = "Sub-título principal"
-var titulos = ["Título principal","Subtitulo 1", "Subtitulo 2", "Subtitulo 3", "Menu"]
-var enlaces=['Acerca', 'Noticias', 'Contacto']

mixin caja(imagen, titulo, contenido)
  .caja
    .caja__imagen: img(src="images/"+imagen, alt="alt")
    .caja__contenido
      h3= titulo
      p= contenido